service ctf
{
    disable = no
    socket_type = stream
    protocol    = tcp
    type        = UNLISTED
    wait        = no
    user        = root
    port        = 9999
    bind        = 0.0.0.0
    server      = /usr/sbin/chroot
    server_args = --userspec=1000:1000 /home/ctf ./distinct.o
    banner_fail = /etc/banner_fail
    per_source	= 10
    rlimit_cpu	= 20
}